render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_lambert, specular_schlick_ggx, world_vertex_coords;


// Varyings
varying vec3 var_pos;

uniform sampler2D Rock : source_color;
uniform sampler2D Grass : source_color;
uniform sampler2D noise : hint_normal;


// TextureParameterTriplanar
	vec4 triplanar_texture(sampler2D p_sampler, vec3 p_weights, vec3 p_triplanar_pos) {
		vec4 samp = vec4(0.0);
		samp += texture(p_sampler, p_triplanar_pos.xy) * p_weights.z;
		samp += texture(p_sampler, p_triplanar_pos.xz) * p_weights.y;
		samp += texture(p_sampler, p_triplanar_pos.zy * vec2(-1.0, 1.0)) * p_weights.x;
		return samp;
	}

	uniform vec3 triplanar_scale = vec3(1.0, 1.0, 1.0);
	uniform vec3 triplanar_offset;
	uniform float triplanar_sharpness = 0.5;

	varying vec3 triplanar_power_normal;
	varying vec3 triplanar_pos;

void vertex() {
// TextureParameterTriplanar
	{
		triplanar_power_normal = pow(abs(NORMAL), vec3(triplanar_sharpness));
		triplanar_power_normal /= dot(triplanar_power_normal, vec3(1.0));
		triplanar_pos = VERTEX * triplanar_scale + triplanar_offset;
		triplanar_pos *= vec3(1.0, -1.0, 1.0);
	}
// Input:3
	vec3 n_out3p0 = VERTEX;


// VaryingSetter:2
	var_pos = n_out3p0;


}

void fragment() {
// TextureParameterTriplanar:2
	vec4 n_out2p0 = triplanar_texture(Rock, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:4
	vec4 n_out4p0 = triplanar_texture(Grass, triplanar_power_normal, triplanar_pos);


// Input:10
	mat4 n_out10p0 = VIEW_MATRIX;


// Input:6
	vec3 n_out6p0 = NORMAL;


// TransformVectorMult:13
	vec3 n_out13p0 = (vec4(n_out6p0, 1.0) * n_out10p0).xyz;


// VaryingGetter:19
	vec3 n_out19p0 = var_pos;


// VectorFunc:16
	vec3 n_out16p0 = normalize(n_out19p0);


// DotProduct:8
	float n_out8p0 = dot(n_out13p0, n_out16p0);


// FloatOp:24
	float n_in24p1 = 1.00000;
	float n_out24p0 = pow(n_out8p0, n_in24p1);


// FloatConstant:21
	float n_out21p0 = 0.950000;


// FloatOp:23
	float n_out23p0 = n_out24p0 - n_out21p0;


// SmoothStep:25
	vec3 n_in25p0 = vec3(0.00000, 0.00000, 0.00000);
	vec3 n_in25p1 = vec3(0.10000, 0.10000, 0.10000);
	vec3 n_out25p0 = smoothstep(n_in25p0, n_in25p1, vec3(n_out23p0));


// Mix:22
	vec3 n_out22p0 = mix(vec3(n_out2p0.xyz), vec3(n_out4p0.xyz), n_out25p0);


// TextureParameterTriplanar:30
	vec4 n_out30p0 = triplanar_texture(noise, triplanar_power_normal, triplanar_pos);


// Output:0
	ALBEDO = n_out22p0;
	NORMAL_MAP = vec3(n_out30p0.xyz);


}
